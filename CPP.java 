#include<iostream>
#include<conio.h>
using namespace std;
class student
{
int reg;
float cgpa;
char name[20];
public:
void input()
{
cout<<"Enter the details";
cin>>reg>>cgpa>>name;
}
void output()
{
cout<<"Display the details";
cout<<reg<<endl<<cgpa<<endl<<name;
}
};
main()
{
int i;
student obj[5];//array of object
for(i=0;i<5;i++)
obj[i].input();
for(i=0;i<5;i++)
obj[i].output();
getch();
}